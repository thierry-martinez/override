(executable
  (name typedtree_collect_texp_apply)
  (preprocess (staged_pps override ppxlib.traverse))
  (libraries compiler-libs.common ppxlib.traverse_builtins))

(rule
  (targets typedtree_collect_texp_apply.ml)
  (deps typedtree_collect_texp_apply.cppo.ml)
  (action
     (run %{bin:cppo} -V OCAML:%{ocaml_version} %{deps} -o %{targets})))
